<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="diary" name="Create Runnable Jar for Project diary">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <!--define folder properties-->
    <property name="dir.buildfile" value="."/>
    <property name="dir.workspace" value="${dir.buildfile}/.."/>
    <property name="dir.jarfile" value="${dir.buildfile}/jar"/>
    <target name="diary">
        <jar destfile="${dir.jarfile}/diary.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="diary.Diary"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="${dir.buildfile}/bin"/>
        </jar>
    </target>
</project>
